{
	"name": "Login",
	"indexHtmlPath": "login.html",
	"classpath": "${app.dir}/app",
	"framework": "ext",
	"theme": "ext-theme-classic",
	"requires": [
		"ext-locale"
	],
	"locale": "ru",
	"sass": {
		"namespace": "Login"
	},
	"url": null,
	"js": [
		{
			"path": "${ext.dir}/build/ext-all-rtl-debug.js"
		},
		{
			"path": "utils.js",
			"includeInBundle": true
		},
		{
			"path": "login.js",
			"bundle": true
		}

	],
	"css": [
        {
            "path": "resources/css/login_style.css",
			"bootstrap": false
        },
        {
            "path": "resources/css/login_fonts.css",
			"bootstrap": false
        },
        {
            "path": "resources/css/login_clear.css",
			"bootstrap": false
        },
		{
			"path": "bootstrap.css",
			"bootstrap": true
		}
	],
	"production": {
	},
	"testing": {
	},
	"development": {
	},
	"bootstrap": {
		"base": "${app.dir}",
		"manifest": "bootstrap.json",
		"microloader": "bootstrap.js",
		"css": "bootstrap.css"
	},
	"resources": [
		"resources/images",
		"resources/fonts"
	],
	"output": {
		"base": "../webapp",
		"page": {
			"path": "login.html",
			"enable": true
		},
		"js": {
			"path": "login.js",
			"enable": true
		},
		"manifest": {
			"path": "login.json",
			"embed": false
		}
	},
	"appCache": {
		"cache": [
			"login.html"
		],
		"network": [
			"*"
		],
		"fallback": []
	},
	"ignore": [
		"(^|/)CVS(/?$|/.*?$)"
	],
	"archivePath": "archive",
	"slicer": {
		"js": [
			{
				"path": "${app.dir}/sass/example/custom.js",
				"isWidgetManifest": true
			}
		]
	},
	"id": "c0e26968-2d78-4cc8-941c-84a01e060e33"
}

